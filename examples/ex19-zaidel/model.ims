begin nonlinear
	outer_hclose 1.e-4
	outer_maximum 100 500 
	under_relaxation dbd
	under_relaxation_theta 0.85
	under_relaxation_kappa 0.100000E-03
	under_relaxation_gamma 0.
	under_relaxation_momentum 0.100000E+00
