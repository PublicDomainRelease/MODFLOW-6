	inner_hclose 1.0e-4
	inner_rclose 0.0 RELATIVE_RCLOSE
	inner_maximum 100
	linear_acceleration bicgstab
	scaling_method none
	REORDERING_METHOD none
	relaxation_factor 0.97
	NUMBER_ORTHOGONALIZATIONS 7
	PRECONDITIONER_LEVELS 5
	PRECONDITIONER_DROP_TOLERANCE 1.0e-5
end linear
